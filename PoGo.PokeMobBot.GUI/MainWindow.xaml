<Window x:Class="PoGo.PokeMobBot.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PoGo.PokeMobBot.GUI"
        mc:Ignorable="d"
        Title="PoGo.PokeMobBot" Height="600" Width="800" MinWidth="800" MinHeight="600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TabControl Grid.Row="0" x:Name="tabSetup" HorizontalAlignment="Stretch" Height="524" Margin="0,0,0,0" VerticalAlignment="Top" >
            <TabItem Header="Status">
                <Grid Background="white">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                        <ScrollViewer HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalScrollBarVisibility="Auto">
                            <StackPanel VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                                <TextBlock x:Name="textblockStatusLog" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" TextWrapping="NoWrap" Margin="0,0,0,0" Text="Placeholder...The raw log from the CLI will scroll in this window"/>
                            </StackPanel>
                        </ScrollViewer>
                    </Grid>
                    <Grid Grid.Row="1" Margin="0,0,0,0" VerticalAlignment="Bottom" HorizontalAlignment="Stretch">
                        <Grid Margin="0,0,0,30" VerticalAlignment="Top" HorizontalAlignment="Stretch">
                            <Grid HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Bottom">
                                <Label x:Name="labelStatusLevel" Content="Level" HorizontalAlignment="Left" Margin="0,30,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold"/>
                                <Label x:Name="labelStatusLevelData" Content="Level data" HorizontalAlignment="Left" Margin="100,30,0,0" VerticalAlignment="Top" Width="100"/>
                                <Label x:Name="labelStatusXP" Content="XP" HorizontalAlignment="Left" Margin="0,56,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold"/>
                                <Label x:Name="labelStatusXPData" Content="XP data" HorizontalAlignment="Left" Margin="100,56,0,0" VerticalAlignment="Top" Width="100"/>
                                <Label x:Name="labelStatusXPNextLevel" Content="XP Next Level" HorizontalAlignment="Left" Margin="0,82,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold"/>
                                <Label x:Name="labelStatusXPNextLevelData" Content="XP NL data" HorizontalAlignment="Left" Margin="100,82,0,0" VerticalAlignment="Top" Width="100"/>
                            </Grid>
                            <Grid HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Bottom">
                                <Label x:Name="labelStatusEXP" Content="EXP/H" HorizontalAlignment="Left" Margin="0,4,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold"/>
                                <Label x:Name="labelStatusEXPData" Content="EXP/H data" HorizontalAlignment="Left" Margin="100,4,0,0" VerticalAlignment="Top" Width="100"/>
                                <Label x:Name="labelStatusPH" Content="P/H" HorizontalAlignment="Left" Margin="0,30,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold"/>
                                <Label x:Name="labelStatusPHData" Content="P/H data" HorizontalAlignment="Left" Margin="100,30,0,0" VerticalAlignment="Top" Width="100"/>
                                <Label x:Name="labelStatusStardust" Content="Stardust" HorizontalAlignment="Left" Margin="0,56,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold"/>
                                <Label x:Name="labelStatusStardustData" Content="Stardust data" HorizontalAlignment="Left" Margin="100,56,0,0" VerticalAlignment="Top" Width="100"/>
                                <Label x:Name="labelStatusTransferred" Content="Transferred" HorizontalAlignment="Left" Margin="0,82,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold"/>
                                <Label x:Name="labelStatusTransferredCount" Content="Transferred data" HorizontalAlignment="Left" Margin="100,82,0,0" VerticalAlignment="Top" Width="100"/>
                            </Grid>
                            <Grid  HorizontalAlignment="Right" Margin="0,0,0,0" VerticalAlignment="Bottom">
                                <Label x:Name="labelStatusRecycled" Content="Recycled" HorizontalAlignment="Left" Margin="0,30,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold"/>
                                <Label x:Name="labelStatusRecycledData" Content="Recycled data" HorizontalAlignment="Left" Margin="100,30,0,0" VerticalAlignment="Top" Width="100"/>
                                <Label x:Name="labelStatusLongitude" Content="Current Longitude" HorizontalAlignment="Left" Margin="0,56,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold"/>
                                <Label x:Name="labelStatusLongitudeData" Content="Longitude data" HorizontalAlignment="Left" Margin="100,56,0,0" VerticalAlignment="Top" Width="100"/>
                                <Label x:Name="labelStatusLatitude" Content="Current Latitude" HorizontalAlignment="Left" Margin="0,82,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold"/>
                                <Label x:Name="labelStatusLatitudeData" Content="Latitude data" HorizontalAlignment="Left" Margin="100,82,0,0" VerticalAlignment="Top" Width="100"/>
                            </Grid>
                        </Grid>
                        <Grid HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Bottom">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Label Grid.Column="0" x:Name="labelStatusPositionDescription" Content="Description" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold"/>
                            <TextBox Grid.Column="1" x:Name="textboxStatusPositionDescriptionData" Height="23" Margin="0,0,0,0" TextWrapping="NoWrap"  Text="This will be the name of the target location that the bot is walking to" VerticalAlignment="Bottom" IsEnabled="False" HorizontalAlignment="Stretch" />
                        </Grid>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Pokemons">
                <Grid Background="white">
                    <ListView x:Name="listviewPokemons" Margin="0,0,0,0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto" BorderThickness="0">
                        <ListView.View>
                            <GridView x:Name="gridviewPokemons">
                                <GridViewColumn x:Name="gridviewPokemonsName" Header="Name" Width="200"/>
                                <GridViewColumn x:Name="gridviewPokemonsLevel" Header="Lvl" Width="40"/>
                                <GridViewColumn x:Name="gridviewPokemonsCP" Header="CP" Width="60"/>
                                <GridViewColumn x:Name="gridviewPokemonsIV" Header="IV" Width="80"/>
                                <GridViewColumn x:Name="gridviewPokemonsMove1" Header="Move1" Width="200"/>
                                <GridViewColumn x:Name="gridviewPokemonsMove2" Header="Move2" Width="200"/>
                            </GridView>
                        </ListView.View>
                    </ListView>
                </Grid>
            </TabItem>
            <TabItem Header="Setup">
                <Grid x:Name="gridSetup" Background="white">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0" VerticalAlignment="Top" HorizontalAlignment="Stretch" Margin="0,0,0,0">
                        <CheckBox x:Name="checkboxUpdateWarning" Content="Update Warning" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                        <Grid VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,5,0,0">
                            <Label x:Name="labelLanguage" Content="Language" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <ComboBox x:Name="comboboxLanguage" HorizontalAlignment="Right" Margin="100,0,0,0" VerticalAlignment="Top" Width="40">
                                <ComboBox.Items>
                                    <ComboBoxItem Name="comboboxLanguage_da" Content="da"/>
                                    <ComboBoxItem Name="comboboxLanguage_de" Content="de"/>
                                    <ComboBoxItem Name="comboboxLanguage_en" Content="en" IsSelected="True"/>
                                    <ComboBoxItem Name="comboboxLanguage_es" Content="es"/>
                                    <ComboBoxItem Name="comboboxLanguage_et" Content="et"/>
                                    <ComboBoxItem Name="comboboxLanguage_fr" Content="fr"/>
                                    <ComboBoxItem Name="comboboxLanguage_gr" Content="gr"/>
                                    <ComboBoxItem Name="comboboxLanguage_id" Content="id"/>
                                    <ComboBoxItem Name="comboboxLanguage_it" Content="it"/>
                                    <ComboBoxItem Name="comboboxLanguage_nl" Content="nl"/>
                                    <ComboBoxItem Name="comboboxLanguage_no" Content="no"/>
                                    <ComboBoxItem Name="comboboxLanguage_pl" Content="pl"/>
                                    <ComboBoxItem Name="comboboxLanguage_ptbr" Content="pt-br"/>
                                    <ComboBoxItem Name="comboboxLanguage_ptpt" Content="pt-pt"/>
                                    <ComboBoxItem Name="comboboxLanguage_ru" Content="ru_RU"/>
                                    <ComboBoxItem Name="comboboxLanguage_sv" Content="sv"/>
                                    <ComboBoxItem Name="comboboxLanguage_th" Content="th"/>
                                    <ComboBoxItem Name="comboboxLanguage_tr" Content="tr"/>
                                    <ComboBoxItem Name="comboboxLanguage_ukua" Content="uk_UA"/>
                                    <ComboBoxItem Name="comboboxLanguage_zhcn" Content="zh_CN"/>
                                    <ComboBoxItem Name="comboboxLanguage_zhtw" Content="zh_tw"/>
                                </ComboBox.Items>
                            </ComboBox>
                        </Grid>
                        <Grid  VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,5,0,0">
                            <Label x:Name="labelWebSocketPort" Content="Web Socket Port" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxWebSocketPort" HorizontalAlignment="Left" Height="23" Margin="150,4,0,5" Text="14251" VerticalAlignment="Top" Width="80"/>
                        </Grid>
                        <Separator HorizontalAlignment="Stretch" Height="5" Margin="0,0,0,0" VerticalAlignment="Bottom"/>
                    </Grid>
                    <Grid Grid.Row="1" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Margin="0,0,0,0">
                        <Grid HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top">
                            <Label x:Name="labelAltitued" Content="Altitude" HorizontalAlignment="Left" Margin="11,6,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxAltitude" HorizontalAlignment="Left" Height="23" Margin="270,10,4,0" Text="10" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelLatitude" Content="Latitude" HorizontalAlignment="Left" Margin="11,32,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxLatitude" HorizontalAlignment="Left" Height="23" Margin="270,36,0,0" Text="38.976226" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelLongitude" Content="Longitude" HorizontalAlignment="Left" Margin="10,58,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxLongitude" HorizontalAlignment="Left" Height="23" Margin="270,62,0,0" Text="-77.643218" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelDelayBetweenPokemonCatch" Content="Delay between catch" HorizontalAlignment="Left" Margin="10,84,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxDelayBetweenPokemonCatch" HorizontalAlignment="Left" Height="23" Margin="270,88,0,0" Text="4000" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelDelayBetweenPlayerActions" Content="Delay Between Actions" HorizontalAlignment="Left" Margin="11,110,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxDelayBetweenPlayerActions" HorizontalAlignment="Left" Height="23" Margin="270,114,0,0" Text="5000" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelMaxTravelDistanceInMeters" Content="Max Travel Distance (m)" HorizontalAlignment="Left" Margin="10,136,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxMaxTravelDistanceInMeters" HorizontalAlignment="Left" Height="23" Margin="270,140,0,0" Text="24000" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelWalkingSpeedInKilometerPerHour" Content="Walking Speed (km/hr)" HorizontalAlignment="Left" Margin="10,162,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxWalkingSpeedInKilometerPerHour" HorizontalAlignment="Left" Height="23" Margin="270,166,0,0" Text="10.0" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelKeepMinCp" Content="Keep Min CP" HorizontalAlignment="Left" Margin="10,188,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxKeepMinCp" HorizontalAlignment="Left" Height="23" Margin="270,192,0,0" Text="1250" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelKeepMinDuplicatePokemon" Content="Keep Min Duplicate Pokemon" HorizontalAlignment="Left" Margin="10,214,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxKeepMinDuplicatePokemon" HorizontalAlignment="Left" Height="23" Margin="270,218,0,0" Text="1" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelKeepMinIvPercentage" Content="Keep Min IV Percentage" HorizontalAlignment="Left" Margin="10,240,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxKeepMinIvPercentage" HorizontalAlignment="Left" Height="23" Margin="270,244,0,0" Text="94.0" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelMaxBallsPerPokemon" Content="Max Balls Per Pokemon" HorizontalAlignment="Left" Margin="10,266,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxMaxBallsPerPokemon" HorizontalAlignment="Left" Height="23" Margin="270,270,0,0" Text="6" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelUseGreatBallAboveIv" Content="Use Great Ball Above IV" HorizontalAlignment="Left" Margin="11,292,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxUseGreatBallAboveIv" HorizontalAlignment="Left" Height="23" Margin="270,296,0,0" Text="750" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelUseMasterBallAboveIv" Content="Use Master Ball Above IV" HorizontalAlignment="Left" Margin="10,318,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxUseMasterBallAboveIv" HorizontalAlignment="Left" Height="23" Margin="270,322,0,0" Text="1500" VerticalAlignment="Top" Width="80"/>
                            <Label x:Name="labelUseUltraBallAboveIv" Content="Use Ultra Ball Above IV" HorizontalAlignment="Left" Margin="11,344,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="textboxUseUltraBallAboveIv" HorizontalAlignment="Left" Height="23" Margin="270,348,0,0" Text="1000" VerticalAlignment="Top" Width="80"/>
                            <CheckBox x:Name="checkboxUseEggIncubators" Content="Use Egg Incubators" HorizontalAlignment="Left" Margin="11,374,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <CheckBox x:Name="checkboxAutoFavoritePokemon" Content="Auto Favorite Pokemon" HorizontalAlignment="Left" Margin="195,374,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <GroupBox x:Name="groupboxTransfer" Header="Transfers" HorizontalAlignment="Left" Margin="3,394,0,0" VerticalAlignment="Top" Width="347">
                                <Grid HorizontalAlignment="Left" Height="38" Margin="0,0,0,0" VerticalAlignment="Top" Width="360">
                                    <CheckBox x:Name="checkboxTransferDuplicatePokemon" Content="Transfer Duplicate Pokemon" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" FontWeight="Bold" IsChecked="True" Checked="checkboxChanged" Unchecked="checkboxChanged" IsEnabledChanged="checkboxIsEnabledChanged"/>
                                    <CheckBox x:Name="checkboxPrioritizeIvOverCP" Content="Prioritize IV over CP" HorizontalAlignment="Left" Margin="8,20,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                                </Grid>
                            </GroupBox>
                        </Grid>
                        <Grid HorizontalAlignment="Right" Margin="0,0,10,0" VerticalAlignment="Top">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <GroupBox Grid.Row="0" x:Name="groupboxEvolution" Header="Evolution" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top">
                                <Grid HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top">
                                    <CheckBox x:Name="checkboxKeepPokemonsThatCanEvolve" Content="Keep Pokemons That Can Evolve" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                                    <CheckBox x:Name="checkboxEvolveAllPokemonWithEnoughCandy" Content="Evolve All Pokemon With Enough Candy" HorizontalAlignment="Left" Margin="0,20,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                                    <CheckBox x:Name="checkboxEvolveAllPokemonAboveIv" Content="Evolve All Pokemon Above IV" HorizontalAlignment="Left" Margin="0,40,0,0" VerticalAlignment="Top" FontWeight="Bold" IsEnabledChanged="checkboxIsEnabledChanged" Checked="checkboxChanged" Unchecked="checkboxChanged"/>
                                    <Label x:Name="labelEvolveAboveIvValue" Content="Evolve Above IV Value" HorizontalAlignment="Left" Margin="10,55,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                                    <TextBox x:Name="textboxEvolveAboveIvValue" HorizontalAlignment="Left" Height="23" Margin="260,59,4,4" Text="90.0" VerticalAlignment="Top" Width="80"/>
                                </Grid>
                            </GroupBox>
                            <GroupBox x:Name="groupboxAutoRenaming" Header="Auto Renaming" HorizontalAlignment="Left" Margin="0,109,0,0" VerticalAlignment="Top">
                                <Grid HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top">
                                    <CheckBox x:Name="checkboxRenamePokemon" Content="Rename Pokemon" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" FontWeight="Bold" Checked="checkboxChanged" Unchecked="checkboxChanged" IsEnabledChanged="checkboxIsEnabledChanged"/>
                                    <CheckBox x:Name="checkboxRenameAboveIv" Content="Rename Above Keep Min IV Percentage" HorizontalAlignment="Left" Margin="14,18,0,0" VerticalAlignment="Top" IsEnabled="False"/>
                                    <Label x:Name="labelRenameTemplate" Content="Rename Template" HorizontalAlignment="Left" Margin="10,31,0,0" VerticalAlignment="Top" FontWeight="Bold" IsEnabled="False"/>
                                    <TextBox x:Name="textboxRenameTemplate" HorizontalAlignment="Left" Height="23" Margin="260,35,4,4" Text="{}{0}_{1}" VerticalAlignment="Top" Width="80" IsEnabled="False"/>
                                </Grid>
                            </GroupBox>
                            <GroupBox x:Name="groupBox3" Header="Lucky Eggs" HorizontalAlignment="Left" Margin="0,191,0,0" VerticalAlignment="Top">
                                <Grid HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top">
                                    <CheckBox x:Name="checkboxUseLuckyEggsWhileEvolving" Content="Use Lucky Eggs While Evolving" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" FontWeight="Bold" Checked="checkboxChanged" Unchecked="checkboxChanged" IsEnabledChanged="checkboxIsEnabledChanged"/>
                                    <Label x:Name="labelUseLuckyEggsMinPokemonAmount" Content="Use Lucky Eggs Min Pokemon Amount" HorizontalAlignment="Left" Margin="10,15,0,0" VerticalAlignment="Top" IsEnabled="False" FontWeight="Bold"/>
                                    <TextBox x:Name="textboxUseLuckyEggsMinPokemonAmount" HorizontalAlignment="Left" Height="23" Margin="260,19,4,4" Text="30" VerticalAlignment="Top" Width="80" IsEnabled="False"/>
                                </Grid>
                            </GroupBox>
                            <GroupBox x:Name="groupGpxPathing" Header="GPX Pathing" HorizontalAlignment="Left" Margin="0,261,0,0" VerticalAlignment="Top">
                                <Grid HorizontalAlignment="Left"  Margin="0,0,0,0" VerticalAlignment="Top">
                                    <CheckBox x:Name="checkboxUseGpxPathing" Content="Use Gpx Pathing" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" FontWeight="Bold" Checked="checkboxChanged" Unchecked="checkboxChanged" IsEnabledChanged="checkboxIsEnabledChanged"/>
                                    <Label x:Name="labelGpxFile" Content="Gpx File Name" HorizontalAlignment="Left" Margin="0,15,0,0" VerticalAlignment="Top" IsEnabled="False" FontWeight="Bold"/>
                                    <TextBox x:Name="textboxGpxFile" HorizontalAlignment="Left" Height="23" Margin="99,16,0,0" Text="" VerticalAlignment="Top" Width="215" IsEnabled="False"/>
                                    <Button x:Name="buttonGpxFilePicker" Content="..." HorizontalAlignment="Left" Margin="322,18,4,0" VerticalAlignment="Top" Width="18" IsEnabled="False" Click="buttonFilenamePicker_Click"/>
                                </Grid>
                            </GroupBox>
                            <GroupBox x:Name="groupboxAuth" Header="Auth" HorizontalAlignment="Left" Margin="0,325,0,0" VerticalAlignment="Top">
                                <Grid HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,0,0">
                                    <Label x:Name="labelAuthType" Content="AuthType" HorizontalAlignment="Left" Margin="10,1,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                                    <ComboBox x:Name="comboboxAuthType" HorizontalAlignment="Left" Margin="260,3,0,0" VerticalAlignment="Top" Width="80" Height="23" SelectionChanged="comboboxSelectionChanged" FontWeight="Bold">
                                        <ComboBoxItem x:Name="comboboxAuthType_Google" Content="Google" IsSelected="True"/>
                                        <ComboBoxItem x:Name="comboboxAuthType_Ptc" Content="PTC"/>
                                    </ComboBox>
                                    <Label x:Name="labelUsername" Content="Username" HorizontalAlignment="Left" Margin="10,27,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                                    <TextBox x:Name="textboxUsername" HorizontalAlignment="Left" Height="23" Margin="260,31,0,0" Text="0" VerticalAlignment="Top"  Width="80"/>
                                    <Label x:Name="labelPassword" Content="Password" HorizontalAlignment="Left" Margin="10,53,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                                    <TextBox x:Name="textboxPassword" HorizontalAlignment="Left" Margin="260,57,0,0" Height="23" VerticalAlignment="Top" FontWeight="Bold" Width="80"/>
                                    <Label x:Name="labelRefreshToken" Content="RefreshToken" HorizontalAlignment="Left" Margin="10,79,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                                    <TextBox x:Name="textboxRefreshToken" HorizontalAlignment="Left" Height="23" Margin="260,83,4,4" Text="0" VerticalAlignment="Top" Width="80"/>
                                </Grid>
                            </GroupBox>
                        </Grid>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="More Setup">
                <Grid x:Name="gridMoreSetup" Background="white">
                    <Grid Grid.Row="1" VerticalAlignment="Top" HorizontalAlignment="Stretch" Margin="10,4,5,4">
                        <Label x:Name="labelMaxSpawnLocationOffset" Content="Max Spawn Location Offset" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                        <TextBox x:Name="textboxMaxSpawnLocationOffset" HorizontalAlignment="Left" Height="23" Margin="260,4,0,0" Text="10" VerticalAlignment="Top" Width="80"/>
                        <Label x:Name="labelLevelUpByCPorIV" Content="Level Up By CP or IV" HorizontalAlignment="Left" Margin="0,26,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                        <ComboBox x:Name="comboboxLevelUpByCPorIV" HorizontalAlignment="Left" Margin="260,30,0,0" VerticalAlignment="Top" Width="80" Height="23">
                            <ComboBoxItem x:Name="comboboxLevelUpByCPorIV_IV" Content="iv"/>
                            <ComboBoxItem x:Name="comboboxLevelUpByCPorIV_CP" Content="cp" IsSelected="True"/>
                        </ComboBox>
                        <Label x:Name="labelUpgradePokemonCpMinimum" Content="Upgrade Pokemon CP Min" HorizontalAlignment="Left" Margin="0,52,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                        <TextBox x:Name="textboxUpgradePokemonCpMinimum" HorizontalAlignment="Left" Height="23" Margin="260,56,0,0" Text="1000" VerticalAlignment="Top" Width="80"/>
                        <Label x:Name="labelUpgradePokemonIvMinimum" Content="Upgrade Pokemon IV Min" HorizontalAlignment="Left" Margin="0,78,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                        <TextBox x:Name="textboxUpgradePokemonIvMinimum" HorizontalAlignment="Left" Height="23" Margin="260,82,0,0" Text="95.0" VerticalAlignment="Top" Width="80"/>

                        <!-- New for 1.0.1 need to reorganize overall list to logically group -->

                        <Label x:Name="labelUseBerryMinCp" Content="Use Berry Min Cp" HorizontalAlignment="Left" Margin="0,104,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                        <TextBox x:Name="textboxUseBerryMinCp" HorizontalAlignment="Left" Height="23" Margin="260,108,0,0" Text="450" VerticalAlignment="Top" Width="80"/>
                        <Label x:Name="labelUseBerryMinIv" Content="Use Berry Min Iv" HorizontalAlignment="Left" Margin="0,130,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                        <TextBox x:Name="textboxUseBerryMinIv" HorizontalAlignment="Left" Height="23" Margin="260,134,0,0" Text="95.0" VerticalAlignment="Top" Width="80"/>
                        <Label x:Name="labelUseGreatBallBelowCatchProbability" Content="Use Great Ball Below Catch Probability" HorizontalAlignment="Left" Margin="0,156,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                        <TextBox x:Name="textboxUseGreatBallBelowCatchProbability" HorizontalAlignment="Left" Height="23" Margin="260,160,0,0" Text="0.5" VerticalAlignment="Top" Width="80"/>
                        <Label x:Name="labelUseUltraBallBelowCatchProbability" Content="Use Ultra Ball Below Catch Probability" HorizontalAlignment="Left" Margin="0,182,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                        <TextBox x:Name="textboxUseUltraBallBelowCatchProbability" HorizontalAlignment="Left" Height="23" Margin="260,186,0,0" Text="0.25" VerticalAlignment="Top" Width="80"/>
                        <Label x:Name="labelUseMasterBallBelowCatchProbability" Content="Use Master Ball Below Catch Probability" HorizontalAlignment="Left" Margin="0,208,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                        <TextBox x:Name="textboxUseMasterBallBelowCatchProbability" HorizontalAlignment="Left" Height="23" Margin="260,212,0,0" Text="0.05" VerticalAlignment="Top" Width="80"/>
                        <!--

                                <ListView.View>
                                    <GridView x:Name="gridviewPokemonToSnipe">
                                        <GridViewColumn x:Name="gridviewPokemonToSnipeLongitude" Header="Longitude" Width="100"/>
                                        <GridViewColumn x:Name="gridviewPokemonToSnipeLattitude" Header="Lattitude Min CP" Width="100"/>
                                        <GridViewColumn x:Name="gridviewPokemonToSnipeLocationName" Header="Location Name" Width="300" />
                                    </GridView>
                                </ListView.View>
                            </ListView>
                        </Grid>
                        <Grid Grid.Column="1" Margin="5,0,10,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                            <Label x:Name="labelPokemonToSnipe" Content="Pokemon to Snipe" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top"/>
                            <TextBox x:Name="textboxPokemonToSnipe" HorizontalAlignment="Stretch" Margin="0,27,0,0" TextWrapping="Wrap" Text="User edits list of pokemons to snipe here" VerticalAlignment="Stretch"/>
                        </Grid>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Not to Catch Filter">
                <Grid x:Name="gridNotToCatchFilter" Background="white">
                    <CheckBox x:Name="checkboxUsePokemonToNotCatchFilter" Content="Use &quot;Pokemon To Not Catch Filter&quot;" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                </Grid>
            </TabItem>
        </TabControl>
        <Grid Grid.Row="1" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Margin="10,0,10,5">
            <Grid VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="0,0,0,0">
                <Button x:Name="buttonStart" Content="Start" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Width="75" Click="buttonClick"/>
                <Button x:Name="buttonContinue" Content="Continue" HorizontalAlignment="Left" Margin="80,0,0,0" VerticalAlignment="Top" Width="75" Click="buttonClick" IsEnabled="False" />
                <Button x:Name="buttonStop" Content="Stop" HorizontalAlignment="Left" Margin="160,0,0,0" VerticalAlignment="Top" Width="75" Click="buttonClick" IsEnabled="False" />
            </Grid>
            <Grid VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="0,0,0,0">
                <Label x:Name="labelStatusRuntime" Content="Runtime" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                <Label x:Name="labelStatusRuntimeData" Content="Runtimedata" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="100,0,0,0"/>
            </Grid>
            <Grid VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,0,0">
                <Button x:Name="buttonApply" Content="Apply" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Width="75" Click="buttonClick"/>
                <Button x:Name="buttonExit" Content="Exit" HorizontalAlignment="Left" Margin="80,0,0,0" VerticalAlignment="Top" Width="75" Click="buttonClick"/>
            </Grid>
        </Grid>
    </Grid>
</Window>
